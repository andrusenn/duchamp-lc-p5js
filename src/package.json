{
      "name": "duchamp-lc",
      "author": {
            "name": "Andres Senn",
            "email": "hola@andressenn.com"
      },
      "license": "MIT",
      "homepage": "http://andressenn.com",
      "version": "0.1.1",
      "description": "p5 livecoding",
      "main": "main.js",
      "scripts": {
            "start": "electron .",
            "dist-win": "electron-builder --win",
            "dist-linux": "electron-builder --linux",
            "dist-mac": "electron-builder --dmg"
      },
      "keywords": [
            "livecoding",
            "p5js",
            "processing"
      ],
      "build": {
            "appId": "com.andrusenn.duchamplc",
            "productName": "Duchamp Livecoding p5js",
            "directories": {
                  "output": "../dist"
            },
            "win": {
                  "target": "zip",
                  "icon": "assets/icon.ico"
            },
            "linux": {
                  "target": [
                        "AppImage",
                        "deb"
                  ],
                  "icon": "assets/icon.png"
            },
            "dmg": {
                  "contents": [
                        {
                              "x": 110,
                              "y": 150
                        },
                        {
                              "x": 240,
                              "y": 150,
                              "type": "link",
                              "path": "/Applications"
                        }
                  ]
            }
      },
      "devDependencies": {
            "electron": "^4.0.8",
            "electron-builder": "^20.38.5"
      },
      "dependencies": {
            "js-beautify": "^1.9.0"
      }
}