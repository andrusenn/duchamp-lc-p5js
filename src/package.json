{
      "name": "leparc-lc",
      "author": {
            "name": "Andres Senn",
            "email": "hola@andressenn.com"
      },
      "license": "MIT",
      "homepage": "http://andressenn.com",
      "version": "0.0.3-alpha",
      "description": "p5 livecoding",
      "main": "main.js",
      "scripts": {
            "start": "electron .",
            "dist-win": "electron-builder --win",
            "dist-linux": "electron-builder --linux",
            "dist-mac": "electron-builder --mac"
      },
      "keywords": [
            "livecoding",
            "p5js",
            "processing"
      ],
      "build": {
            "appId": "com.andrusenn.leparc",
            "productName": "LeParc-LC",
            "directories": {
                  "output": "../dist"
            },
            "win": {
                  "target": "zip",
                  "icon": "assets/icon.ico"
            },
            "linux": {
                  "target": [
                        "AppImage"
                  ],
                  "icon": "assets/icon.png"
            },
            "mac": {
                  "target": "zip",
                  "icon": "assets/icon.icns"
            }
      },
      "devDependencies": {
            "electron": "^4.0.8",
            "electron-builder": "^20.38.5"
      },
      "dependencies": {
            "express": "^4.16.4",
            "ip": "^1.1.5",
            "js-beautify": "^1.9.0",
            "socket.io": "^2.2.0"
      }
}
